public class LogicalOperator {

 public static void main(String[] args) {
  // And(&&) Operator
  System.out.println((5 < 6) && (5 < 6));
  System.out.println((4 < 5) && (4 > 5));

  // OR(||) Operator
  System.out.println((5 < 3) || (8 > 5));
  System.out.println((5 > 3) || (8 < 5));
  System.out.println((5 < 3) || (8 < 5));

  // not(!)Operator
  System.out.println(!(5 == 3));
  System.out.println(!(5 > 3));
 }
}
