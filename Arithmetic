public class Arithmetic {

 int a = 5;
 int b = 15; // instance variable

 int add() { // non parameterized method
  int c = a + b;
  System.out.println(c);
  return c;
 }

 int sub() { // non parameterized method
  int c = a - b;
  System.out.println(c);
  return c;
 }

 public static void main(String[] args) {
  Arithmetic frist = new Arithmetic();
  frist.add();
  frist.sub();
 }
}
