public class Average_Of_Array {

 public static void main(String[] args) {

  // create an array

  int a[] = { 10, 29, 38, 47, 56 };

  int average = 0;
  int sum = 0;
  for (int i = 0; i < a.length; i++) {
   sum = sum + a[i];
  }
  average = sum / a.length;
  System.out.println("average of an element = " + average);
 }
}
